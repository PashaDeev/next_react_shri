{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "target": "ES5",
    "noEmit": false,
    "allowJs": true,
    "module": "commonjs",
    "outDir": ".next/production-server/",
    "esModuleInterop": true,
    "lib": [
      "es2019"
    ]
  },
  "include": ["./src/**/*.ts", "./src/**/*.js"],
  "exclude": ["test/**/*.ts"]
}
